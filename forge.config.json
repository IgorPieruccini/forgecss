{
  "sourceDir": "./examples/",
  "entry": "./examples/index.jsx",
  "outDir": "./forge.css",
  "fileExtensions": [
    "jsx",
    "tsx"
  ],
  "variant": {
    "px": [
      "2",
      "4",
      "8"
    ]
  },
  "alias": {
    "margin": {
      "margin": "m",
      "margin-bottom": "mb",
      "margin-top": "mt",
      "margin-left": "ml",
      "margin-right": "mr"
    },
    "padding": {
      "padding": "p",
      "padding-bottom": "pb",
      "padding-top": "pt",
      "padding-left": "pl",
      "padding-right": "pr"
    },
    "flex": "flex",
    "justify-content": "justify-to",
    "flex-direction": "flex-dir",
    "align-items": "align-to"
  },
  "resolve": {
    "alias": {
      "App": "./src/App.js",
      "state": "./src/state",
      "shared": "./src/shared",
      "authentication": "./src/authentication",
      "support": "./src/support",
      "test": "./src/test",
      "elearning": "./src/elearning",
      "gamification": "./src/gamification",
      "account": "./src/account"
    }
  }
}
