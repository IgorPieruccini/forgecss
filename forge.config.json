{
  "sourceDir": "./examples/",
  "outDir": "./forge.css",
  "fileExtensions": [
    "jsx",
    "tsx"
  ],
  "variant": {
    "size": {
      "px": [
        2,
        4,
        8
      ]
    }
  },
  "alias": {
    "margin": {
      "margin": "m",
      "margin-bottom": "mb",
      "margin-top": "mt",
      "margin-left": "ml",
      "margin-right": "mr"
    },
    "padding": {
      "padding": "p",
      "padding-bottom": "pb",
      "padding-top": "pt",
      "padding-left": "pl",
      "padding-right": "pr"
    },
    "flex": "flex",
    "justify-content": "jc"
  }
}
